import java.util.Scanner;
public class Task1
{
	public static void main(String[] args) {
		System.out.print("Введите число: ");
		Scanner in = new Scanner(System.in);
        int num = in.nextInt();
		if (num==7) {
		    System.out.println("Привет");
		}

		else {
		   System.out.println ("Программа завершена");
		}
	}
}
